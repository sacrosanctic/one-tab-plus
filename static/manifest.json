{
  "name": "My first Svelte Extension",
  "description": "A browser extension made with Svelte Kit",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "scripting",
    "contextMenus"
  ],
  "action": {
    "default_title": "Svelte Extension",
    "default_icon": "favicon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type":"module"
  }
}